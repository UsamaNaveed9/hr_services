 "css": ".header {\n    display: flex;\n    text-align: right;\n}\n.customer-div {\n    display: flex;\n    text-align: right;\n}\n.first-div {\n    width: 10%;\n}\n.second-div {\n    width: 50%;\n}\n.three-div {\n    width: 15%;\n}\n.four-div {\n    width: 25%;\n}\n.left-div {\n    width: 17%;\n}\n.middle-div {\n    width: 63%;\n    padding-right: 8px;\n}\n.right-div {\n    padding-right: 0px;\n    padding-left: 0px;\n    width: 20%;\n    padding-top: 10px;\n    padding-left: 8px;\n}\n.print-format .table-bordered td {\n    border: 1px solid #000000;\n}\n.print-format td {\n    padding: 3px !important;\n}\n.file-preview {\n    display: flex;\n    flex-wrap: wrap;\n}\n.file-preview img, .file-preview iframe {\n    width: 300px; /* Set a fixed width for the preview items */\n    height: auto;\n    margin: 10px;\n}",
