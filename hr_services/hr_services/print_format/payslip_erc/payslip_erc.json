 "doctype": "Print Format",
 "font_size": 14,
 "html": "<table width=\"100%\">\r\n    <thead>\r\n    <tr width=\"100%\">\r\n        <td width=\"100%\">\r\n            <div class=\"header-space\"></div>\r\n            <div class=\"header\">\r\n                {% if letter_head %}\r\n                {{letter_head}}\r\n                {% endif %}\r\n            </div>\r\n        </td>\r\n    </tr>\r\n    </thead>\r\n\r\n    <tbody width=\"100%\">\r\n    <tr width=\"100%\">\r\n        <td width=\"100%\" style=\"padding: 0!important;\">\r\n            <div class=\"content\">\r\n                <div class=\"invoice-table-div\">\r\n                    <table style=\"margin: 0; margin-bottom: 10px;\" class=\"table table-bordered\">\r\n                        <tr>\r\n                            <td style=\"background-color: darkgray;\">\r\n                                <h3 style=\"margin: 0;font-size: 22px;\" class=\"text-center\">Pay Slip</h3>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>\r\n                                <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px; width: 17%;\">\r\n                                        <p>Pay Month :</p>\r\n                                        <p>Worked Days :</p>\r\n                                        <p>Department :</p>\r\n                                        <p>Designation :</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 5px;padding-left: 0px; width: 27%;\">\r\n                                        {% set date_object = frappe.utils.getdate(doc.start_date) %}\r\n                                        {% set month_name = date_object.strftime('%B') %}\r\n                                        {% set year = date_object.strftime('%Y') %}\r\n                                        <p>{{month_name}}-{{year}}</p>\r\n                                        <p>{{doc.total_working_days|int}}</p>\r\n                                        <p>{{doc.project_name}}</p>\r\n                                        <p>{{frappe.db.get_value(\"Employee\",doc.employee, \"designation\") or \"\"}}</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 5px; width: 22%;\">\r\n                                        <p>Date of Joining :</p>\r\n                                        <p>Employee ID :</p>\r\n                                        <p>National ID/Iqama :</p>\r\n                                        <p>Employee Name :</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;width: 34%;\">\r\n                                        {% set date_of_joining = frappe.db.get_value(\"Employee\", doc.employee, \"date_of_joining\") %}\r\n                                        {% if date_of_joining %}\r\n                                            {% set formatted_date = frappe.utils.formatdate(date_of_joining, 'dd-MM-yyyy') %}\r\n                                        {% endif %}    \r\n                                        <p>{{formatted_date}}</p>\r\n                                        <p>{{doc.employee}}</p>\r\n                                        <p>{{ frappe.db.get_value(\"Employee\", doc.employee, \"iqama_national_id\") }}</p>\r\n                                        <p>{{frappe.db.get_value(\"Employee\",doc.employee,\"employee_name\")}}<br>{{ frappe.db.get_value(\"Employee\", doc.employee, \"name_in_arabic\") }}</p>\r\n                                    </div>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                    </table>\r\n                    <table style=\"margin: 0; margin-bottom: 10px;\" class=\"table table-bordered\">\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"background-color: darkgray;\">\r\n                                <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px; \" class=\"text-left\">Earnings</h3>\r\n                                    </div>\r\n                                    <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px\" class=\"text-center\">Remarks</h3>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px\" class=\"text-right\">Amount</h3>\r\n                                    </div>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                {% for row in doc.earnings %}\r\n                                <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <p class=\"text-left\">{{row.salary_component}}</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <p class=\"text-center\">{{row.remarks or \"&nbsp;\"}}</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <p class=\"text-right\">{{row.get_formatted(\"amount\")}}</p>\r\n                                    </div>\r\n                                </div>\r\n                                {% endfor %}\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td style=\"width:70%\">\r\n                                <p class=\"text-right\"><b>Total Earnings</b></p>\r\n                            </td>\r\n                            <td>\r\n                                <p class=\"text-right\" style=\"padding-right: 6px;\"><b>{{doc.get_formatted(\"gross_pay\")}}</b></p>\r\n                            </td>\r\n                        </tr>\r\n                    </table>\r\n                    <table style=\"margin: 0; margin-bottom: 10px;\" class=\"table table-bordered\">\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"background-color: darkgray;\">\r\n                                <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px; \" class=\"text-left\">Deductions</h3>\r\n                                    </div>\r\n                                    <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px\" class=\"text-center\">Remarks</h3>\r\n                                    </div>\r\n                                    <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <h3 style=\"margin: 0;font-size: 18px\" class=\"text-right\">Amount</h3>\r\n                                    </div>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                {% if doc.deductions %}\r\n                                    {% for row in doc.deductions %}\r\n                                    <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                        <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-left\">{{row.salary_component}}</p>\r\n                                        </div>\r\n                                        <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-center\">{{row.remarks or \"&nbsp;\"}}</p>\r\n                                        </div>\r\n                                        <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-right\">{{row.get_formatted(\"amount\")}}</p>\r\n                                        </div>\r\n                                    </div>\r\n                                    {% endfor %}\r\n                                {% endif %}\r\n                                {% if doc.loans %}\r\n                                    {% for row in doc.loans %}\r\n                                    <div class=\"col-xs-12\" style=\"padding-right: 6px; padding-left: 6px;\">\r\n                                        <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-left\">{{row.loan_type}}</p>\r\n                                        </div>\r\n                                        <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-center\">{{ \"&nbsp;\" }}</p>\r\n                                        </div>\r\n                                        <div class=\"col-xs-3\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                            <p class=\"text-right\">{{row.get_formatted(\"total_payment\")}}</p>\r\n                                        </div>\r\n                                    </div>\r\n                                    {% endfor %}\r\n                                {% endif %}\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>\r\n                                <p class=\"text-right\"><b>Total Deductions</b></p>\r\n                            </td>\r\n                            <td>\r\n                                {% set total_sum = doc.total_deduction + doc.total_loan_repayment %}\r\n                                <p class=\"text-right\" style=\"padding-right: 6px;\"><b>{{ frappe.format(total_sum, {'fieldtype': 'Currency'}) }}</b></p>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td style=\"width:70%\">\r\n                                <p class=\"text-right\"><b>Net Pay</b></p>\r\n                            </td>\r\n                            <td>\r\n                                <p class=\"text-right\" style=\"padding-right: 6px;\"><b>{{doc.get_formatted(\"net_pay\")}}</b></p>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" >\r\n                                <div style=\"padding-top: 65px;\">\r\n                                    <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <p class=\"text-center\">Employer Signature</p>\r\n                                    </div>\r\n                                    <div class=\"col-xs-6\" style=\"padding-right: 0px;padding-left: 0px;\">\r\n                                        <p class=\"text-center\">Employee Signature</p>\r\n                                    </div>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                    </table>\r\n                </div>\r\n            </div>\r\n        </td>\r\n    </tr>\r\n    </tbody>\r\n\r\n    <tfoot class=\"#tfoot\">\r\n    <tr>\r\n        <td>\r\n\r\n            <div class=\"footer\">\r\n                <div class=\"col-xs-12\">\r\n                    <div class=\"footer-img\">\r\n                        <img src=\"/files/comfooter.png\">\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"footer-space\"></div>\r\n\r\n        </td>\r\n    </tr>\r\n    </tfoot>\r\n</table>\r\n",
